# 系统思维智能体项目 - 环境变量模板
# 复制此文件为 .env 并根据需要修改

# 应用环境
NODE_ENV=development
RUST_LOG=info
JAVA_OPTS=-Xmx512m

# 数据库配置
POSTGRES_DB=mydb
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_URL=redis://localhost:6379

# 服务端口配置
GUI_PORT=3000
RUST_BACKEND_PORT=8001
JAVA_BACKEND_PORT=8002
PYTHON_AGENT_PORT=8003
JAVA_AGENT_PORT=8004
ORCHESTRATION_PORT=8005

# 数据库连接URL
DATABASE_URL=postgresql://postgres:password@localhost:5432/mydb
RUST_DATABASE_URL=postgres://postgres:password@localhost:5432/mydb
JAVA_DATABASE_URL=jdbc:postgresql://localhost:5432/mydb

# 日志配置
LOG_LEVEL=DEBUG
LOG_FILE=logs/agentic-systemthinking.log

# 连接池配置
DB_POOL_SIZE=10
DB_CONNECTION_TIMEOUT=30
DB_IDLE_TIMEOUT=300

# 服务配置
SERVICE_TIMEOUT=30
HEALTH_CHECK_INTERVAL=30
MAX_RETRY_ATTEMPTS=3

# 智能体配置
AGENT_HEALTH_CHECK_INTERVAL=60
AGENT_TIMEOUT=120
AGENT_MAX_RETRY=3

# 编排服务配置
ORCHESTRATION_HEALTH_CHECK_INTERVAL=30
ORCHESTRATION_TIMEOUT=60
ORCHESTRATION_CIRCUIT_BREAKER_THRESHOLD=5
